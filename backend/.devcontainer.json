{
  "name": "backend container",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "backend",
  "workspaceFolder": "/workspace",
  "shutdownAction": "none",
  "settings": {
    "go.useLanguageServer": true,
    "[go]": {
      "editor.tabSize": 4,
      "editor.insertSpaces": false,
      "editor.codeActionsOnSave": {
        "source.organizeImports": true
      }
    }
  },
  "extensions": [
    "golang.go"
  ]
}
